   "source": [
    "def arrayList(arr, target, index, lista):\n",
    "    if index == len(arr)-1:\n",
    "        return lista\n",
    "    if target == arr[index]:\n",
    "        lista.append(index)\n",
    "    return arrayList(arr, target, index + 1, lista)"
   ]
