    "def phone(p, up):\n",
    "    if len(up) == 0:\n",
    "        print(p)\n",
    "        return\n",
    "    \n",
    "    digit = int(up[0])\n",
    "    \n",
    "    for i in range((digit - 1)*3,digit*3):\n",
    "        ch = chr(97+i)\n",
    "        phone(p + ch, up[1:])\n",
    "        \n",
    "phone(\" \",\"158\")"
   ]
  }
