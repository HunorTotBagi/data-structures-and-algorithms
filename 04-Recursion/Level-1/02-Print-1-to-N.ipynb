    "# Print all numbers from 1 to n with recursion.\n",
    "\n",
    "def Print_Recursion(n):\n",
    "    if n == 0:\n",
    "        return\n",
    "    Print_Recursion(n-1)\n",
    "    print(n) # When the recursion call is ended than print.\n",
    "        \n",
    "Print_Recursion(5)"
   ]
  }
