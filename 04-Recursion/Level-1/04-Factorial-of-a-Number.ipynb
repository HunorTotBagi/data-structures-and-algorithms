   "source": [
    "# Return the factorial of a number.\n",
    "\n",
    "def factorial(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    return n * factorial(n-1)\n",
    "    \n",
    "factorial(5)"
   ]
  }
