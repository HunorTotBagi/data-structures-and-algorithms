   "source": [
    "#### In a 3x3 matrix print the ways to reach the end (2,2) from start (0,0) by only going right and down."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "9a20eaca",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "6"
      ]
     },
     "execution_count": 7,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def ways(r, c):\n",
    "    if r == 1 or c == 1:\n",
    "        return 1\n",
    "    return ways(r - 1, c) + ways(r, c - 1)\n",
    "\n",
    "ways(3,3)"
   ]
  },
